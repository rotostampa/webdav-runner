{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "plugins": ["eslint-plugin-unused-imports", "eslint-plugin-simple-import-sort"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "unused-imports/no-unused-imports": "error",
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error"
    }
}
